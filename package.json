{
  "name": "file-assistant",
  "version": "1.2.1",
  "description": "This is a stable and tested file-system package to create, copy, move, merge and overwrite files and folders, to empty folders and to write, modify or append content to the files, according to the given [Array] or JSON structure instructions.",
  "main": "index.js",
  "scripts": {
    "test": "node tests/jasmine.conf.js",
    "open-dir": "start ."
  },
  "keywords": [
    "create",
    "mkdir",
    "file",
    "files",
    "folder",
    "folders",
    "copy",
    "paste",
    "move",
    "append",
    "content",
    "contents",
    "directory",
    "overwrite",
    "fs",
    "file-system",
    "file system",
    "recursive",
    "recurse",
    "dir",
    "appendfile",
    "writefile",
    "ensuledir",
    "ensurefile"
  ],
  "author": "Paweł Rafałko",
  "license": "MIT",
  "dependencies": {
    "cli-color": "^1.2.0",
    "empty-folder": "^2.0.1",
    "list-contents": "^4.0.1",
    "move-on": "^1.0.0",
    "of-type": "^2.1.0",
    "typeof-arguments": "^3.1.0",
    "typeof-properties": "^1.1.1"
  },
  "devDependencies": {
    "fs-extra": "^4.0.0",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1"
  }
}
